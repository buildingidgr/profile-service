{
  "name": "mechhub-api",
  "version": "1.0.0",
  "description": "MechHub Profile Service",
  "main": "dist/api-service.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/api-service.js",
    "start:api": "node dist/api-service.js",
    "start:consumer": "node dist/consumer/consumer-service.js",
    "dev:api": "nodemon src/api-service.ts",
    "dev:consumer": "nodemon src/consumer/consumer-service.ts",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:consumer\"",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "railway:up": "railway up",
    "railway:run": "railway run npm start",
    "generate-lockfile": "npm install --package-lock-only"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "amqplib": "^0.10.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "nodemailer": "^6.9.3",
    "prisma": "^4.16.2",
    "redis": "^4.6.6",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.2",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
